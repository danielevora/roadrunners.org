</section>

<script type="text/javascript">
	var public_spreadsheet_url = 'https://docs.google.com/spreadsheet/pub?key=0AgF3RXbmZJcbdHpRVjE2R2RSOFcxNHluR3JBdENiZmc&output=html';

	$(document).ready( function() {
		Tabletop.init({ key: public_spreadsheet_url,
		                     callback: showInfo,
		                     simpleSheet: true,
		                     parseNumbers: true
		                     
		});
	});

  function showInfo(data, tabletop) {
  	//tabletop.sheets("2014");
  	var source = $("#handlebars-template").html();
		var template = Handlebars.compile(source);
  	
  	//tabletop.model.toArray();
    //alert("Successfully processed!");
    //console.log(data);
    
    $.each( tabletop.sheets("2014").all(), function(i, cat) {
		  var html = template(cat);
		  $("#grandprix").append(html);
		});
  }
</script>

<script id="handlebars-template" type="text/x-handlebars-template">
		<li>{{details_2}}</li>
</script>

